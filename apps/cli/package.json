{
  "name": "sde-converter",
  "version": "0.1.0",
  "description": "EVE SDE Converter to JSON",
  "main": "index.ts",
  "types": "index.ts",
  "type": "module",
  "repository": "https://github.com/eve-graph/sde-converter.git",
  "author": "Jo√£o Neto <joao@neto.pt>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit ./index.ts && esbuild ./index.ts --bundle --platform=node --format=cjs --outfile=./dist/index.js",
    "generate": "mkdir -p out && ts-node index.ts generate",
    "validate": "ts-node index.ts validate",
    "rclone:sync": "rclone sync ./out sde2.jita.space:/jitaspace-sde -P",
    "rclone:delete": "rclone delete sde2.jita.space:/jitaspace-sde -P"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@jsonhero/schema-infer": "^0.1.4",
    "@openapi-contrib/json-schema-to-openapi-schema": "^2.2.5",
    "ansi-colors": "^4.1.3",
    "change-case": "^4.1.2",
    "checksum": "^1.0.0",
    "cli-progress": "^3.12.0",
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "js-yaml": "^4.1.0",
    "js2schema": "^1.0.8",
    "node-stream-zip": "^1.15.0",
    "yaml": "^2.1.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jitaspace/eslint-config": "*",
    "@types/cli-progress": "^3.11.0",
    "@types/figlet": "^1.5.6",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.16.4",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
}
